      "translation": {
        "en": {
          "short": "regular polygon",
          "long": "a two-dimensional closed shape with straight sides and equal angles between adjacent sides, and all sides have the same length",
          "partOfSpeech": "noun"
        },
        "id": {
          "short": "poligon beraturan",
          "long": "Bentuk tertutup dua dimensi dengan sisi lurus dan sudut yang sama di antara sisi yang berdekatan, dan semua sisi memiliki panjang yang sama",
          "partOfSpeech": "kata benda"
        }
      },
